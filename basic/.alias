alias ll='ls -la --color=auto'
alias ls='ls --color=auto'
alias sudo='sudo -E '

alias timestamp='date +%s -d '

if [ -f .alias_customized ]; then
    source .alias_customized
fi

if [ -f .alias_docker ]; then
    source .alias_docker
fi

if [ -f .bash_common ]; then
    source .bash_common
fi

if [ -f .alias_common ]; then
    source .alias_common
fi



